page{
  width: 100%;
}
.imagesize{
 display:flex;
 height: 600px;                    
 justify-content: center;        
 align-items:center;               
}

.imagesize image { 
  width:900rpx;
  height:500rpx;
  }
  
.titleContainer{
  /* position: absolute; */
  width: 100%;
}
.big-title {
  position: absolute;
  color:rgb(53, 133, 238);
  font-size: 40rpx;
  padding: 100rpx;
  left:5%;
}


.input-container {
  display: flex;
  flex-direction: column;
  align-items: center;
  flex-wrap: wrap;
  background-color: #fff;
  width: 720rpx;
  margin-left: 15rpx;
  margin-right: 15rpx;
  font-size: 44rpx;
  box-sizing: border-box;
  box-shadow: 4px 1px 1px #ccc;
  position: relative;
  padding-bottom: 100rpx;
}

/* border-radius: 98rpx;是控制按钮边变圆 */

.goodbutton {
  position: absolute;
  left:10%;
  bottom: 5%;
  margin-top: 30px;
  background-color: rgb(37, 163, 5);
  color: white;
  border-radius: 98rpx;
  background: bg_red;
}
/* 按下变颜色 */
.hover {
  background:  rgb(156, 236, 236);
}

.btn {
  position: absolute;
  right: 5%;
  bottom: 15%;
  width: 160rpx!important;
  font-size: 26rpx;
  border-radius: 10rpx;
  background: #337ab7;
  color: #fff;
  padding: 0!important;
  min-height: 30rpx!important;
  height: 60rpx;
  line-height: 60rpx;
}

.algo-title {
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  font-size: 36rpx;
}

.line {
  width: 720rpx;
  height: 3rpx;
  background: rgba(0, 0, 0, 0.096);
}

.input-item {
  display: flex;
  flex-direction: row;
  justify-content: space-evenly;
  align-items: center;
  margin-top: 30rpx;
  height: 100rpx;
  width: 720rpx;
}

.param-name {
  font-size: 32rpx;
  width: 300rpx;
}

.input {
  width: 300rpx;
  height: 100rpx;
  border: 1px solid rgba(0, 0, 0, 0.096);
  border-radius: 25rpx;
}

.return{
  position: absolute;
  right: 5%;
  top: 100rpx;
  width: 140rpx;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 10rpx;
  text-align: center;
  background: #337ab7;
  color:#fff;
}
.tr {
  display: flex;
  width: 100%;
  justify-content: space-evenly;
  align-items: center;
  height: 80rpx;
}
.td {
    width:80%;
    text-align: center;
}
.th {
  width: 80%;
  background: #337ab7;
  color: #fff;
  text-align: center;
}
.history-table-wrap{
  position: absolute;
  width: 668rpx;
  height: 800rpx;
  left: 50%;
  margin-left: -334rpx;
  top: 70rpx;
  overflow-y: scroll;
  overflow-x: hidden;
}