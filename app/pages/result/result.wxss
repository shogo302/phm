/* page设置当前窗口背景色 */
page{
  background-color: #F5F5F5
}

/* 定义.container设置容器样式 */
.container{
  padding: 10px 20px;
  align-content: center;
}

/* 定义.text-content设置文本样式 */
.text-content{
  padding: 10px;
  align-content: center;
  border-bottom: 1rpx solid #F5F5F5;
  background-color: #FFFFFF
}

.goodbutton1 {
  position: absolute;
  left:10%;
  bottom: 15%;
  margin-top: 30px;
  background-color: rgb(240, 146, 6);
  color: white;
  border-radius: 98rpx;
  background: bg_red;
}
/* 按下变颜色 */
.hover {
  background:  rgb(156, 236, 236);
}
.tr {
  display: flex;
  width: 100%;
  justify-content: space-evenly;
  align-items: center;
  height: 80rpx;
}
.td {
    width:80%;
    text-align: center;
}
.th {
  width: 80%;
  background: #337ab7;
  color: #fff;
  text-align: center;
}
.history-table-wrap{
  position: absolute;
  width: 668rpx;
  height: 800rpx;
  left: 50%;
  margin-left: -334rpx;
  top: 70rpx;
  overflow-y: scroll;
  overflow-x: hidden;
}